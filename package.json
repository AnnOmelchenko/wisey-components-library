{
  "name": "wisey-components-library",
  "version": "0.0.3",
  "description": "Components Library React Typescript Sass",
  "targets": {
    "css": {
      "source": "src/styles/main.scss"
    },
    "main": {
      "source": "src/index.tsx"
    },
    "types": {
      "source": "src/index.tsx"
    }
  },
  "css": "dist/main.css",
  "source": "src/index.tsx",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "prepare": "npm run build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaOstrovska/wisey-components-library.git"
  },
  "keywords": [
    "Components",
    "Library",
    "React",
    "Typescript",
    "Sass"
  ],
  "author": "Anna Ostrovska",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnnaOstrovska/wisey-components-library/issues"
  },
  "homepage": "https://github.com/AnnaOstrovska/wisey-components-library#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "parcel": "^2.8.3",
    "sass": "^1.62.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
